{
  "name": "@hackersheet/react-document-content",
  "version": "0.1.0-alpha.1",
  "description": "Hacker Sheet document content component for React.",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/hackersheet/javascript.git",
    "directory": "packages/react-document-content"
  },
  "license": "MIT",
  "author": "",
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./style": {
      "import": "./dist/style.module.css",
      "require": "./dist/style.module.css",
      "types": "./dist/style.module.scss.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf ./dist && pnpm tsup && pnpm typegen:scss && pnpm build:css",
    "build:css": "cp src/styles/*.ts dist && pnpm sass src/styles/style.module.scss dist/style.module.css --no-source-map",
    "check": "pnpm tsc && pnpm lint",
    "format": "pnpm -w format && pnpm lint:fix",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{ts,tsx}\"",
    "pub": "pnpm publish --access=public --tag alpha",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typegen:scss": "typed-scss-modules \"src/styles/style.module.scss\""
  },
  "dependencies": {
    "@hackersheet/core": "alpha",
    "deepmerge": "^4.0.0",
    "mustache": "^4.0.0",
    "react-markdown": "^9.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "remark-directive": "^3.0.0",
    "remark-directive-rehype": "^0.4.2",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@hackersheet/eslint-config-custom": "workspace:*",
    "@types/hast": "3.0.4",
    "@types/mustache": "4.2.5",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/unist": "3.0.2",
    "postcss": "8.4.39",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "1.77.8",
    "typed-scss-modules": "8.0.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
